{
    "name": "nahid/apiz",
    "description": "Apiz is a boilerplate for REST API HTTP call manager",
    "type": "php",
    "require": {
        "php": ">=5.6.0 | >=7.0",
        "ext-json": "*",
        "lib-libxml" : ">=2.6.20",
        "nahid/qarray": "^2.0",
        "guzzlehttp/guzzle": "^6.0 | ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symfony/var-dumper": "^5.1"
    },
    "suggest": {
        "ext-curl": "*",
        "ext-libxml": "*",
        "ext-yaml": "*",
        "ext-simplexml" : "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nahid Bin Azhar",
            "email": "nahid.dns@gmail.com"
        },
        {
            "name": "Ahmed shamim",
            "email": "shaon.cse81@gmail.com"

        }
    ],
    "autoload": {
      "psr-4": {
            "Apiz\\": "src/",
            "Tests\\": "Tests/"
      },
      "files": [
      ]
    },
    "minimum-stability": "dev",
    "autoload-dev": {
        "psr-4": {
            "Api\\" : "api/"
        }
    },
    "prefer-stable": true
}
