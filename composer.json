{
    "name": "nahid/apiz",
    "description": "Apiz is a boilerplate for REST API manager",
    "type": "php",
    "require": {
        "php": ">=5.5.9 | >=7.0",
        "guzzlehttp/guzzle": "^6.4",
        "ext-json": "*",
        "lib-libxml" : ">=2.6.20",
        "nahid/qarray": "^1.2",
        "guzzlehttp/psr7": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "symfony/var-dumper": "^3.4"
    },
    "suggest": {
        "ext-curl": "*",
        "ext-libxml": "*",
        "ext-yaml": "*",
        "ext-simplexml" : "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nahid Bin Azhar",
            "email": "nahid.dns@gmail.com"
        }
    ],
    "autoload": {
      "psr-4": {
            "Apiz\\": "src/"
      },
      "files": [
      ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
